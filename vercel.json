{
  "buildCommand": "pnpm run build",
  "installCommand": "corepack enable && corepack prepare pnpm@9.0.6 --activate && pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:slug([a-zA-Z0-9_-]+)",
      "destination": "/api/resolve/:slug"
    }
  ],
  "functions": {
    "app/api/resolve/[slug]/route.ts": {
      "maxDuration": 10
    },
    "app/api/redirect/[slug]/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production",
    "COREPACK_ENABLE_STRICT": "0"
  }
}
